<Window x:Class="SmashApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SmashApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="1400">
    <Grid Margin="0,0,0,-16">
        <Image x:Name="Logo" HorizontalAlignment="Left" Margin="376,46,0,0" VerticalAlignment="Top" Source="/Images/SmashApp1.png" Height="575" Width="654"/>
        <Button x:Name="MatchReporting" Content="Match Reporting" HorizontalAlignment="Left" Margin="308,679,0,0" VerticalAlignment="Top" Height="69" Width="332" FontSize="36" FontFamily="MS Reference Sans Serif" Background="#FFE00B03" Foreground="White" Click="MatchReporting_Click"/>
        <Button x:Name="User_Profile" Content="User Profile" HorizontalAlignment="Left" Margin="739,679,0,0" VerticalAlignment="Top" Height="69" Width="323" FontSize="36" FontFamily="MS Reference Sans Serif" Background="#FFE00B03" Foreground="White" Click="User_Profile_Click"/>
        <Label x:Name="SmashApp" Content="Smash App" HorizontalAlignment="Center" Margin="0,454,0,0" VerticalAlignment="Top" Height="232" Width="760" FontSize="150"/>

    </Grid>
</Window>
